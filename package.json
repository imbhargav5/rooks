{
  "name": "rooks",
  "version": "5.0.2",
  "description": "Useful React hooks for everyone.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "unpkg": "dist/umd/rooks.umd.js",
  "types": "dist/types/index.d.ts",
  "typings": "dist/types/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/"
  ],
  "scripts": {
    "clean": "rimraf dist lib",
    "commit": "cz",
    "lint": "eslint src/**/*.ts",
    "format": "yarn run prettier --write \"./src/*.+(js|jsx|ts|tsx)\" && yarn run prettier --write \"./docs/*.+(md|mdxx)\"",
    "test": "jest",
    "coverage": "yarn test --coverage",
    "create": "node helpers/create",
    "new": "yarn run create",
    "prebuild": "rimraf dist",
    "compute-package-list": "node helpers/compute-package-list",
    "build:custom-github-actions": "cd ./.github/actions && for d in ./*/ ; do (cd \"$d\" && npm install && npm run build); done",
    "build:esm": "tsc -m esNext --outDir dist/esm",
    "build:cjs": "tsc -m commonjs --outDir dist/cjs",
    "build:umd": "rollup -c rollup.config.js",
    "build:types": "rollup -c dts.rollup.config.js",
    "build": "yarn build:esm && yarn build:cjs && yarn build:umd && yarn build:types",
    "generate:changelog": "gulp changelog",
    "release-dev": "np prerelease --tag=dev",
    "docs:examples": "docusaurus-examples",
    "docs:start": "docusaurus start",
    "docs:build": "docusaurus build",
    "docs:clear": "docusaurus clear",
    "docs:publish-gh-pages": "docusaurus-publish",
    "docs:write-translations": "docusaurus-write-translations",
    "docs:version": "docusaurus docs:version",
    "docs:rename-version": "docusaurus-rename-version",
    "docs:deploy": "docusaurus deploy",
    "docs:swizzle": "docusaurus swizzle",
    "docs:docusaurus": "docusaurus",
    "docs:serve": "docusaurus serve"
  },
  "dependencies": {
    "js-cookie": "^2.2.1",
    "lodash.debounce": "^4.0.8",
    "raf": "^3.4.1"
  },
  "devDependencies": {
    "@babel/cli": "7.5.5",
    "@babel/core": "7.13.15",
    "@babel/plugin-external-helpers": "7.2.0",
    "@babel/plugin-proposal-export-default-from": "7.5.2",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-react": "7.0.0",
    "@docusaurus/core": "^2.0.0-alpha.75",
    "@docusaurus/preset-classic": "^2.0.0-alpha.75",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-multi-entry": "^4.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-typescript": "^6.1.0",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.1",
    "@testing-library/react-hooks": "^5.1.2",
    "@types/jest": "^26.0.22",
    "@types/js-cookie": "^2.2.6",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@widgetbot/embed-api": "^1.1.3",
    "@widgetbot/react-embed": "^1.3.0",
    "assert": "^2.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "8.0.5",
    "babel-plugin-prismjs": "1.0.2",
    "babel-preset-minify": "0.5.1",
    "browser-env": "3.2.5",
    "camel-case": "^4.1.1",
    "chalk": "2.4.2",
    "classnames": "^2.2.6",
    "clsx": "^1.1.1",
    "conventional-changelog-cli": "^2.1.1",
    "cz-conventional-changelog": "3.3.0",
    "esbuild": "^0.11.12",
    "eslint": "^7.24.0",
    "eslint-config-canonical": "^25.9.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-testing-library": "^4.1.0",
    "execa": "^4.1.0",
    "fs-extra": "^9.1.0",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "gulp-conventional-changelog": "^2.0.35",
    "husky": "^4.2.5",
    "inquirer": "7.0.0",
    "jest": "^26.6.3",
    "jest-localstorage-mock": "^2.4.3",
    "lint-staged": "^10.2.12",
    "lodash.capitalize": "4.2.1",
    "lodash.sortby": "^4.7.0",
    "lodash.truncate": "^4.4.2",
    "make-dir": "1.3.0",
    "mdast-normalize-headings": "^2.0.0",
    "mdast-util-compact": "^3.0.0",
    "mdast-util-from-markdown": "^0.8.1",
    "mdast-util-heading-range": "^2.1.5",
    "mdast-util-to-markdown": "^0.5.3",
    "mdast-zone": "^4.0.1",
    "meow": "5.0.0",
    "mini-css-extract-plugin": "0.8.0",
    "np": "^7.4.0",
    "ora": "4.0.3",
    "pkg-dir": "^5.0.0",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "16.13.0",
    "read-pkg-up": "7.0.0",
    "readline-sync": "^1.4.10",
    "remark": "^13.0.0",
    "remark-behead": "^2.3.3",
    "remark-frontmatter": "^3.0.0",
    "remark-preset-lint-markdown-style-guide": "^4.0.0",
    "remark-strip-badges": "^5.0.0",
    "replace-string": "2.0.0",
    "rollup": "^2.33.3",
    "rollup-plugin-dts": "^1.4.14",
    "rollup-plugin-esbuild": "^4.0.1",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "shelljs": "0.8.3",
    "terser": "^5.6.1",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5",
    "unist-util-find": "^1.0.2",
    "unist-util-find-all-after": "^3.0.2",
    "unist-util-find-all-before": "^3.0.1",
    "unist-util-find-all-between": "^2.1.0",
    "unist-util-visit": "^2.0.3",
    "vfile-reporter": "^6.0.1",
    "webpack": "4.46.0",
    "webpack-cli": "^4.2.0",
    "write-pkg": "3.2.0",
    "wsrun": "^5.2.4",
    "yargs": "^16.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "if [[ $IS_GH_ACTION != 1 ]]; then lint-staged; fi;"
    }
  },
  "lint-staged": {
    "*.{html,js,ts,tsx,json,yml,css,scss}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
